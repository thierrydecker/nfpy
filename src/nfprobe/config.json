{
  "adapters": {
    "eth0": {
      "description": "Ethernet adapter",
      "timeout_ms": 100
    },
    "eth1": {
      "description": "Ethernet adapter",
      "timeout_ms": 100
    }
  },
  "loggers": {
    "formatters": {
      "brief": {
        "format": "%(message)s"
      },
      "verbose": {
        "format": "%(asctime)s %(name)-12s %(levelname)-8s %(message)s"
      }
    },
    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "formatter": "verbose",
        "level": "DEBUG"
      }
    },
    "root": {
      "handlers": [
        "console"
      ],
      "level": "DEBUG"
    }
  }
}